{
    "name": "gatsby-starter-hello-world",
    "private": true,
    "description": "A simplified bare-bones starter for Gatsby",
    "version": "0.1.0",
    "license": "0BSD",
    "scripts": {
        "build": "gatsby build",
        "develop:gatsby": "gatsby develop",
        "develop:type-check": "tsc --watch",
        "develop": "npm-run-all --parallel develop:gatsby , develop:type-check",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "start": "npm run develop",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
    },
    "dependencies": {
        "@mdx-js/mdx": "^1.6.22",
        "@mdx-js/react": "^1.6.22",
        "gatsby": "^3.9.0",
        "gatsby-awesome-pagination": "^0.3.8",
        "gatsby-plugin-image": "^1.9.0",
        "gatsby-plugin-mdx": "^2.9.0",
        "gatsby-plugin-sass": "^4.9.0",
        "gatsby-plugin-sharp": "^3.9.0",
        "gatsby-source-filesystem": "^3.9.0",
        "gatsby-transformer-remark": "^4.6.0",
        "gatsby-transformer-sharp": "^3.9.0",
        "nord": "^0.2.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-icons": "^4.2.0",
        "sass": "^1.35.2"
    },
    "devDependencies": {
        "npm-run-all": "^4.1.5",
        "prettier": "2.3.2",
        "ts-node": "^9.1.1",
        "typescript": "^4.3.5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
    },
    "bugs": {
        "url": "https://github.com/gatsbyjs/gatsby/issues"
    }
}
